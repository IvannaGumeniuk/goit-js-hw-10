<div class="country-cards-tamplate">
<div class="card-img-top">
  <img src="${{ flags.svg }}" alt="${{ name.official }}">
</div>

<div class="card-body">
  <h2 class="card-title">повна назва країни: ${{ name.official }}</h2>
  <p class="card-text">столиця: ${{ capital }}</p>
  <p class="card-text">населення: ${{ population }}</p>
  
  <p class="card-text">масив мов: ${{ languages }}</p>
  
  <ul>
  {{#each languages}}
  <li class="languages-list">${{ languages }}</li>
    {{#each}}
  </ul>

</div>
</div>